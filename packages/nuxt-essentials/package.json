  "devDependencies": {
    "@nuxt/eslint": "latest",
    "@nuxt/kit": "^4.1.2",
    "@nuxtjs/i18n": "^10.1.0",
    "@portfolio/eslint": "workspace:^",
    "@portfolio/oxlint": "workspace:^",
    "@portfolio/prettier": "workspace:^",
    "@sentry/nuxt": "^10.12.0",
    "@types/minimist": "^1.2.5",
    "@vueuse/nuxt": "^13.9.0",
    "consola": "^3.4.2",
    "es-main": "^1.4.0",
    "eslint": "^9.35.0",
    "minimist": "^1.2.8",
    "nuxt": "catalog:",
    "nuxt-security": "^2.4.0",
    "typescript": "^5.9.2",
    "vue": "catalog:"
